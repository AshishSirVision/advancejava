		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>




	<servlet>
		<servlet-name>HomePage</servlet-name>
		<servlet-class>HomePage</servlet-class>
		<init-param>
			<param-name>Username</param-name>
			<param-value>Tom</param-value>
