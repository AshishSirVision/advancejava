		<init-param>
			<param-name>Username</param-name>
			<param-value>Tom</param-value>
		</init-param>
		<init-param>
			<param-name>Password</param-name>
			<param-value>Tom</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>DashBoradPage</servlet-name>
		<servlet-class>DashBoradPage</servlet-class>
		<init-param>
			<param-name>Username</param-name>
			<param-value>Tom</param-value>
		</init-param>
		<init-param>
			<param-name>Password</param-name>
			<param-value>Tom</param-value>
		</init-param>
	</servlet>
<context-param>
   <param-name>WebsiteName</param-name>
   <param-value>advantech</param-value>
</context-param>

<context-param>
   <param-name>WURL</param-name>
   <param-value>advantecheducation</param-value>
</context-param>
<context-param>
